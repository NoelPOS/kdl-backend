{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install --legacy-peer-deps",
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/dist/main.js"
    }
  ]
}
